import java.util.Scanner;
public class Main 
{
    public static void main(String[] args) 
    {
        Scanner s = new Scanner(System.in);
	System.out.print("Enter number:");
	int n=s.nextInt();
	System.out.print("Enter number:"); 
	int a=s.nextInt();
        for(int i=1; i <= a; i++)
        {
            System.out.println(n+" * "+i+" = "+n*i);
        }
    
}
}
